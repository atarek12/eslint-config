{
	"name": "@ljharb/eslint-config",
	"version": "19.0.0",
	"author": "Jordan Harband",
	"funding": {
		"url": "https://github.com/sponsors/ljharb"
	},
	"description": "My shareable eslint config",
	"license": "MIT",
	"main": "index.js",
	"scripts": {
		"find-unused": "eslint-find-rules --unused",
		"prepublishOnly": "safe-publish-latest && npm test && npm run find-unused",
		"prepublish": "not-in-publish || npm run prepublishOnly",
		"pretest": "eclint check *",
		"test": "npm run --silent tests-only",
		"posttest": "npx aud --production",
		"tests-only": "eslint index.js && node .",
		"version": "auto-changelog && git add CHANGELOG.md",
		"postversion": "auto-changelog && git add CHANGELOG.md && git commit --no-edit --amend && git tag -f \"v$(node -e \"console.log(require('./package.json').version)\")\""
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/ljharb/eslint-config.git"
	},
	"keywords": [
		"eslint",
		"eslint-config"
	],
	"devDependencies": {
		"aud": "^1.1.5",
		"auto-changelog": "^2.3.0",
		"eclint": "^2.8.1",
		"eslint": "^8.2.0",
		"eslint-find-rules": "^4.0.0",
		"in-publish": "^2.0.1",
		"safe-publish-latest": "^2.0.0",
		"tape": "^5.3.1"
	},
	"peerDependencies": {
		"eslint": "^8.2.0"
	},
	"engines": {
		"node": ">= 12 || ^10.12"
	},
	"publishConfig": {
		"access": "public"
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "keepachangelog",
		"unreleased": false,
		"commitLimit": false,
		"backfillLimit": false,
		"hideCredit": true,
		"startingVersion": "v19.0.0"
	}
}
